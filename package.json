{
  "name": "Ninix",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm test -s"
  },
  "dependencies": {
    "apisauce": "^0.14.0",
    "format-json": "^1.0.3",
    "immutability-helper": "^2.6.2",
    "js-base64": "^2.4.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "querystringify": "^1.0.0",
    "ramda": "^0.24.1",
    "react": "^16.2.0",
    "react-native": "^0.51",
    "react-native-animatable": "^1.2.3",
    "react-native-ble-plx": "^0.6.1",
    "react-native-charts-wrapper": "^0.4.0",
    "react-native-config": "^0.6.0",
    "react-native-device-info": "^0.11.0",
    "react-native-drawer": "^2.3.0",
    "react-native-image-picker": "^0.26.4",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-pages": "^0.6.1",
    "react-native-sentry": "^0.32.0",
    "react-native-textinput-effects": "0.4.2",
    "react-native-vector-icons": "^4.3.0",
    "react-navigation": "^1.0.0-beta.30",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.6",
    "realm": "^2.0.13",
    "redux": "^3.7.2",
    "redux-persist": "^4.8.3",
    "redux-saga": "^0.15.6",
    "reduxsauce": "^0.6.0",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "2.0.0",
    "enzyme": "^2.9.1",
    "flow-bin": "0.45.0",
    "husky": "^0.14.3",
    "ignite-ir-boilerplate": "^2.0.2",
    "jest": "^20.0.4",
    "mockery": "^2.1.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^16.0.0-alpha.12",
    "reactotron-react-native": "^1.12.2",
    "reactotron-redux": "^1.12.2",
    "reactotron-redux-saga": "^1.12.2"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "config": {}
}
