{
  "name": "Ninix",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm test -s"
  },
  "dependencies": {
    "antd-mobile": "^2.1.8",
    "apisauce": "^0.14.3",
    "format-json": "^1.0.3",
    "immutability-helper": "^2.6.6",
    "js-base64": "^2.4.3",
    "lodash": "^4.17.5",
    "lottie-react-native": "^2.5.0",
    "moment": "^2.22.0",
    "node-libs-react-native": "^1.0.2",
    "prop-types": "^15.6.1",
    "querystringify": "^1.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.1",
    "react-native": "^0.54.4",
    "react-native-animatable": "^1.2.4",
    "react-native-ble-plx": "^0.9.0",
    "react-native-charts-wrapper": "^0.4.3",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.21.4",
    "react-native-drawer": "^2.5.0",
    "react-native-elements": "^1.0.0-beta4",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-image-picker": "^0.26.7",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-nordic-dfu": "^1.1.2",
    "react-native-notifications": "^1.1.19",
    "react-native-pages": "^0.7.0",
    "react-native-sentry": "^0.35.3",
    "react-native-textinput-effects": "^0.4.2",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.10",
    "react-navigation-redux-helpers": "^1.0.3",
    "react-redux": "^5.0.7",
    "realm": "^2.3.3",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "ignite-ir-boilerplate": "^2.0.2",
    "jest": "^22.4.3",
    "jsdom": "^11.8.0",
    "jsdom-global": "^3.0.2",
    "mockery": "^2.1.0",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.0.0-alpha.12",
    "reactotron-react-native": "^1.12.2",
    "reactotron-redux": "^1.12.2",
    "reactotron-redux-saga": "^1.12.2",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/jest.setup.js"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/jest.setup"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "config": {}
}
